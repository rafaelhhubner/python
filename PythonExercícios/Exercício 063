print('-=' * 30)
print('                    \033[1mSequência Fibonacci\033[m')
print('-=' *30)
num = int(input('Quantos termos você quer ver? '))
n1 = 0
n2 = 1
contador = 3
print(f'{n1} -> {n2}', end='')
while contador <= num:
    n3 = n1 + n2
    print(f' -> {n3}', end='')
    n1 = n2
    n2 = n3
    contador += 1
print(' -> ACABOU')
