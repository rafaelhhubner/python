maior = 0
menor = 0
a = 0
for c in range(1,6):
    a = a+ + 1
    peso = float(input(f'Qual o {a}Â° peso? '))
    if c == 1:
        maior = peso
        menor = peso
    else:
        if peso > maior:
           maior = peso
        if peso < menor:
            menor = peso
print(f'O maior peso lido foi {maior}')
print(f'O menor peso lido foi {menor}')
